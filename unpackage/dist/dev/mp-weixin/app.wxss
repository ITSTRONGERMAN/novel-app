
	/*每个页面公共css */
.status-bar{
	height: var(--status-bar-height);
}
*{
	box-sizing: border-box;
}
.bcg-color{
	background-color: #F6F6F6;
}
.bcg-color-white{
	background-color: #fff;
}
@font-face {
	font-family: "custom-icon";
	/* Project id 4716732 */
	src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');
}
.custom-icon {
	font-family: "custom-icon" !important;
	font-size: 16px;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.custom-icon-battery-full:before {
	content: "\e642";
}
.custom-icon-battery-mid:before {
	content: "\e639";
}
.custom-icon-battery-most:before {
	content: "\e9f2";
}
.custom-icon-battery-low:before {
	content: "\e622";
}
.icon-battery-empty:before {
	content: "\e637";
}
.custom-icon-bookshelfshujia:before {
	content: "\e84a";
}
page{--status-bar-height:25px;--top-window-height:0px;--window-top:0px;--window-bottom:0px;--window-left:0px;--window-right:0px;--window-magin:0px}[data-c-h="true"]{display: none !important;}